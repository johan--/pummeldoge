<%= form_for @movie do |f| %>
  <%= f.label :organization_permalink %>
  <%= f.text_field :organization_permalink %><br />

  <%= f.label :project_name %>
  <%= f.text_field :project_name %><br />

  <%= f.label :folder_path %>
  <%= f.text_field :folder_path %><br />

  <%= f.label :file_name %>
  <%= f.text_field :file_name %><br />

  <%= f.submit %>
<% end %>